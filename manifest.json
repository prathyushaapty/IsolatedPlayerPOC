{
    "manifest_version": 2,
    "name": "Basic Iframe Extensions",
    "description": "",
    "version": "1.0",
    "browser_action": {
        "default_title": "test"
    },
    "permissions": [
        "activeTab",
        "storage",
        "clipboardWrite",
        "tabs",
        "cookies",
        "system.memory",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self';",
    "web_accessible_resources": ["*", "letznav-frame.js", "letznav-player.html", "iframe-internal.js"],
    "content_scripts": [{
        "matches": ["http://111.93.27.187:8889/*","https://www.concursolutions.com/*", "https://ap4.salesforce.com/*","https://ap4.lightning.force.com/*","https://success.salesforce.com/communitySetupModule/*", "https://appexchange.salesforce.com/setupModule/*"],
        "js": ["js/main-window.js", "js/iframe.js"],
        "css": ["css/main-window.css"],
        "run_at": "document_idle",
        "all_frames": true
    }]
}